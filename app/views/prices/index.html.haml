%div#main{ role: 'main' }
  #main-content
    .container
      .row
        .col-sm-12
          .page-header.page-header-with-icon.no-mg-t
            %i.fa-icon-suitcase
            %h2
              = @page.title
          .row
            .col-sm-12
              != @page.body
              - unless @page.body.blank?
                %hr
    = render partial: '/prices/list', locals: { prices: @prices }
